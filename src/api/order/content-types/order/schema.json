{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Заказы",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "integer"
    },
    "orderId": {
      "type": "string"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Ожидается оплата",
        "Оплачен",
        "Отменен"
      ]
    },
    "products": {
      "type": "component",
      "repeatable": true,
      "component": "components.product-list"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "address": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "firstName": {
      "type": "string"
    },
    "secondName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "promocode": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::promocode.promocode"
    }
  }
}
